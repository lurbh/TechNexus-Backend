{{#extends "base"}}

{{#block "content"}}
<h1>Create New Order</h1>
<form method="POST">
    <input type="hidden" name="_csrf" value="{{csrfToken}}"/>
    {{{form}}}
    <a class="btn btn-danger mt-3" href="/admin/orders">Cancel</a>
    <input type="submit" class="btn btn-primary mt-3" value="Add Order"/>
</form>
{{/block}}

{{/extends}}